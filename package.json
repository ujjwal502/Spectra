{
  "name": "spectra",
  "version": "0.3.0",
  "description": "AI-powered automation regression testing agent for APIs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "generate": "ts-node src/index.ts generate",
    "generate:ai": "ts-node src/index.ts generate --ai",
    "generate:spec": "ts-node src/index.ts generate-spec",
    "generate:spec:backend": "ts-node src/index.ts generate-spec examples/backend ./examples/backend/enhanced-schema.json",
    "generate:spec:decision-services": "ts-node src/index.ts generate-spec examples/rp-ccs-decision-services-ao--hk-hbap-develop ./examples/rp-ccs-decision-services-ao--hk-hbap-develop/enhanced-schema.json",
    "generate:spec:ds-ao": "ts-node src/index.ts generate-spec examples/rp-ccs-ds-ao--hk-hbap-develop ./examples/rp-ccs-ds-ao--hk-hbap-develop/enhanced-schema.json",
    "generate:spec:ds-router": "ts-node src/index.ts generate-spec examples/rp-ccs-ds-ao-router-hcc--hk-hbap-develop ./examples/rp-ccs-ds-ao-router-hcc--hk-hbap-develop/enhanced-schema.json",
    "generate:spec:mock-api": "ts-node src/index.ts generate-spec examples/mock-api ./examples/mock-api/openapi.json",
    "generate:spec:demo-api": "ts-node src/index.ts generate-spec examples/demo-api ./examples/demo-api/openapi.json",
    "run": "ts-node src/index.ts run",
    "run:ai": "ts-node src/index.ts run --ai",
    "regression:baseline": "ts-node src/index.ts regression:baseline --ai",
    "regression:run": "ts-node src/index.ts regression:run --ai",
    "example:file-upload": "ts-node examples/file-upload-test.ts",
    "test:backend": "ts-node examples/backend-test.ts",
    "test:backend:regression:baseline": "ts-node examples/backend-test.ts --save-baseline",
    "test:backend:regression": "ts-node examples/backend-test.ts --regression",
    "test:mock-api": "ts-node examples/mock-api-test.ts",
    "test:mock-api:regression:baseline": "ts-node examples/mock-api-test.ts --save-baseline",
    "test:mock-api:regression": "ts-node examples/mock-api-test.ts --regression",
    "dashboard": "node src/ui/start-demo.js",
    "demo": "node src/ui/start-demo.js",
    "backend": "node start-backend.js",
    "enhanced": "ts-node src/cli/enhanced.ts",
    "enhanced:info": "ts-node src/cli/enhanced.ts info",
    "enhanced:check": "ts-node src/cli/enhanced.ts check-env",
    "enhanced:demo": "ts-node src/cli/enhanced.ts demo-enhanced",
    "enhanced:generate": "ts-node src/cli/enhanced.ts generate-spec-enhanced",
    "enhanced:dashboard": "ts-node src/index.ts generate-dashboard",
    "demo:dashboard": "node examples/dashboard-demo.js",
    "test:mock-api:enhanced": "npm run test:mock-api:enhanced:generate && npm run test:mock-api:enhanced:execute",
    "test:mock-api:enhanced:generate": "ts-node src/index.ts generate-tests-enhanced examples/mock-api/openapi.json ./examples/mock-api/enhanced-tests",
    "test:mock-api:enhanced:execute": "ts-node src/index.ts execute-tests-intelligent ./examples/mock-api/enhanced-tests http://localhost:8080",
    "test:mock-api:enhanced:workflow": "ts-node src/index.ts test-workflow-enhanced ./examples/mock-api http://localhost:8080 ./examples/mock-api/spectra-enhanced-tests",
    "test:demo-api:enhanced": "npm run test:demo-api:enhanced:generate && npm run test:demo-api:enhanced:execute",
    "test:demo-api:enhanced:generate": "ts-node src/index.ts generate-tests-enhanced examples/demo-api/openapi.json ./examples/demo-api/enhanced-tests",
    "test:demo-api:enhanced:execute": "ts-node src/index.ts execute-tests-intelligent ./examples/demo-api/enhanced-tests http://localhost:8081",
    "test:demo-api:enhanced:workflow": "ts-node src/index.ts test-workflow-enhanced ./examples/demo-api http://localhost:8081 ./examples/demo-api/spectra-enhanced-tests",
    "test:demo-api:intelligent": "ts-node src/cli/enhanced.ts run-intelligent-testing examples/demo-api/openapi.json",
    "test:mock-api:intelligent": "ts-node src/cli/enhanced.ts run-intelligent-testing examples/mock-api/openapi.json"
  },
  "keywords": [
    "api",
    "testing",
    "automation",
    "gherkin",
    "regression",
    "ai",
    "curl"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^12.0.1",
    "@faker-js/faker": "^9.7.0",
    "@langchain/core": "^0.3.61",
    "@langchain/langgraph": "^0.3.5",
    "@langchain/openai": "^0.5.16",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.8.4",
    "commander": "^14.0.0",
    "cucumber": "^6.0.7",
    "dotenv": "^16.5.0",
    "form-data": "^4.0.0",
    "gherkin-parse": "^1.0.12",
    "jest": "^29.7.0",
    "openai": "^4.93.0",
    "openapi-parser": "^0.2.0",
    "openapi-types": "^12.1.3",
    "repomix": "^0.3.9",
    "swagger-parser": "^10.0.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.5.3"
  }
}
