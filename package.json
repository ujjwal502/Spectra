{
  "name": "spectra",
  "version": "0.3.0",
  "description": "AI-powered API testing automation for demo-api",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "generate:spec:demo-api": "ts-node src/index.ts generate-spec examples/demo-api ./examples/demo-api/enrichedOpenapi.json",
    "test:demo-api:intelligent": "ts-node src/cli/enhanced.ts run-intelligent-testing examples/demo-api/openapi.json"
  },
  "keywords": [
    "api",
    "testing",
    "automation",
    "gherkin",
    "regression",
    "ai",
    "curl"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^12.0.1",
    "@faker-js/faker": "^9.7.0",
    "@langchain/core": "^0.3.61",
    "@langchain/langgraph": "^0.3.5",
    "@langchain/openai": "^0.5.16",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.8.4",
    "commander": "^14.0.0",
    "cucumber": "^6.0.7",
    "dotenv": "^16.5.0",
    "form-data": "^4.0.0",
    "gherkin-parse": "^1.0.12",
    "jest": "^29.7.0",
    "openai": "^4.93.0",
    "openapi-parser": "^0.2.0",
    "openapi-types": "^12.1.3",
    "repomix": "^0.3.9",
    "swagger-parser": "^10.0.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.5.3"
  }
}
